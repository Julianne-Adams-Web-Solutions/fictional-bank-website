{
  "extends": ["next", "next/core-web-vitals", "eslint:recommended", "airbnb", "airbnb-typescript", "airbnb/hooks", "prettier"],
  "parserOptions": {
    "project": "./tsconfig.json"
  },
  "globals": {
    "React": "readonly"
  },
  "rules": {
    "no-unused-vars": "warn",
    "react/jsx-props-no-spreading": "off",
    "react/function-component-definition": [2, { "namedComponents": "arrow-function" }]
  }
}